.wave {
  opacity: .4;
  position: absolute;
  top: -35%;
  left: 50%;
  background: #0af;
  width: 500px;
  height: 500px;
  margin-left: -250px;
  margin-top: -250px;
  -webkit-transform-origin: 50% 48%;
          transform-origin: 50% 48%;
  border-radius: 43%;
  -webkit-animation: drift 8000ms infinite linear;
          animation: drift 8000ms infinite linear;
}

.wave.-three {
  -webkit-animation: drift 6500ms infinite linear;
          animation: drift 6500ms infinite linear;
}

.wave.-two {
  -webkit-animation: drift 8000ms infinite linear;
          animation: drift 8000ms infinite linear;
  opacity: .1;
  background: yellow;
}

.box:after {
  content: '';
  display: block;
  left: 0;
  top: 0;
  width: 100%;
